@model Shopping.Data.Entities.Country

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input type="hidden" asp-for="States" />

            <partial name="_Country" />

            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-sm btn-primary" />
                <a asp-action="Index" class="btn btn-sm btn-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
